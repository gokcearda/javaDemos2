package classesWithAttributes;

public class Product {

	// attribute ya da field olarak adland覺r覺lan k覺s覺m
	int _id;
	String name;
	String description;
	double price;
	int stockAmount;
	String renk;
	String kod;

	// getter
	public int getId() {
		return _id;
	}

	// setter
	public void setId(int id) {
		_id = id;
	}

}
